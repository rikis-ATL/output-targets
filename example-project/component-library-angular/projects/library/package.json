{
  "name": "component-library-angular",
  "private": true,
  "peerDependencies": {
    "@angular/common": "^20.0.0",
    "@angular/core": "^20.0.0"
  },
  "files": [
    "dist",
    "package.json",
    "src"
  ],
  "dependencies": {
    "component-library": "workspace:*",
    "tslib": "^2.3.0"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/fesm2022/component-library-angular.mjs"
    },
    "./src/directives/my-button": {
      "types": "./src/directives/my-button.ts",
      "default": "./dist/fesm2022/component-library-angular.mjs"
    },
    "./src/directives/my-checkbox": {
      "types": "./src/directives/my-checkbox.ts", 
      "default": "./dist/fesm2022/component-library-angular.mjs"
    },
    "./src/directives/*": {
      "types": "./src/directives/*.ts",
      "default": "./dist/fesm2022/component-library-angular.mjs"
    }
  }
}
